pipeline {
  agent any 

  stages {
      stage ('Check OUT') {
        steps {
            git branch: 'main', credentialsId: 'Mamita123', url:  'https://github.com/Mamita123/Week5_JenkinsAssignment.git'
         
        }

      }
     stage ('') {
        steps {
           bat 'mvn clean install'
        }

      }

   
      }
  }



